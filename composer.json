{
  "name": "alsc/streambus",
  "description": "Redis Streams message bus",
  "version": "0.2",
  "type": "library",
  "license": "MIT",
  "keywords": [ "redis", "stream", "message", "message bus", "event bus", "queue"],
  "authors": [
    {
      "name": "Alexander Schastny",
      "email": "al.schastny@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "predis/predis": "^3.4",
    "psr/log": "^1.1|^2|^3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "monolog/monolog": "*",
    "phpunit/phpunit": "^11.4",
    "psalm/phar": "^6.12",
    "symfony/process": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "StreamBus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StreamBus\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
